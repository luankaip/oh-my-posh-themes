{
  "blocks": [
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "diamond",
          "foreground": "#100e23",
          "background": "#EEEEEE",
          "leading_diamond": "<#E8DC1F,>\uE0B2\uE0D2</>\uE0B2",
          "trailing_diamond": "\uE0B0<#E8DC1F,>\uE0D4\uE0B0</>",
