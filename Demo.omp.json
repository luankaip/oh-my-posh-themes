          }
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#100e23",
          "background": "#91ddff",
          "powerline_symbol": "\uE0B0",
          "properties": {
			"prefix": "\uF7A1 ",
            "postfix": "",
            "display_status": true,
            "display_stash_count": true,
            "display_upstream_icon": true,
            "status_colors_enabled": true,
            "color_background": false,
            "local_changes_color": "",
            "working_color": "#E84855",
            "staging_color": "#2FDA4E",
            "ahead_color": "#2EC4B6",
            "behind_color": "#8A4FFF",
            "git_icon": "\uE255"
