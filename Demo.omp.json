          }
        },
        {
          "type": "path",
          "style": "powerline",
          "foreground": "#193549",
          "background": "#95ffa4",
          "powerline_symbol": "\uE0B0",
          "properties" : {
			"prefix": "\uF414 ",
            "postfix": "",
            "home_icon": "\uF015 ",
            "folder_icon": "\uF413",
            "folder_separator_icon": "\u27E9",
