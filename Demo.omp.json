          "style": "diamond",
          "foreground": "#DCDCCC",
          "background": "#424242",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "\uE0B0",
          "properties": {
			"prefix": "<#7CFC00,>\uF872</> ",
            "postfix": "",
