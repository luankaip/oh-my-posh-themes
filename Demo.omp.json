{
  "final_space": true,
  "blocks": [
    {
       "type": "prompt",
       "alignment": "right",
       "segments": [
	    {
          "type": "exit",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#00897b",
          "leading_diamond": "\uE0B6\uE0B4\uE0D4",
          "trailing_diamond": "\uE0D2",
          "properties": {
            "display_exit_code": true,
            "always_enabled": true,
            "color_background": true,
            "error_color": "#FF6480",
			"always_numeric": true,
			"success_icon": "\uF441 SUCCESS",
			"error_icon": "\uF188 ERROR:"
          }
        },
		{
          "type": "os",
          "style": "plain",
          "foreground": "#26C6DA",
          "properties": {
            "prefix": "",
            "macos": "\uF179 macOS",
			"linux": "\uF17C Linux",
			"windows": "\uF17A Windows",
			"display_distro_name": true,
			"alpine": "\uF300 Alpine",
			"aosc": "\uF301 AOSC",
			"arch": "\uF303 Arch",
			"centos": "\uF304 CentOS",
			"coreos": "\uF305 CoreOS",
			"debian": "\uF306 Debian",
			"devuan": "\uF307 Devuan",
			"raspbian": "\uF315 Raspbian",
			"elementary": "\uF309 Elementary",
			"fedora": "\uF30A Fedora",
			"gentoo": "\uF30D Gentoo",
			"mageia": "\uF310 Mageia",
			"manjaro": "\uF312 Manjaro",
			"mint": "\uF30E Mint",
			"nixos": "\uF313 NixOS",
			"opensuse": "\uF314 openSUSE",
			"sabayon": "\uF317 Sabayon",
			"slackware": "\uF319 Slackware",
			"ubuntu": "\uF31B Ubuntu"
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "foreground": "#100e23",
          "background": "#EEEEEE",
          "leading_diamond": "<#E8DC1F,>\uE0B2\uE0D2</>\uE0B2",
          "trailing_diamond": "\uE0B0<#E8DC1F,>\uE0D4\uE0B0</>",
          "properties": {
            "prefix": "\uF7D9 ",
            "postfix": "",
            "style": "roundrock",
            "always_enabled": true
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#007ACC",
          "properties": {
            "prefix": " \uF64F ",
            "postfix": "",
            "time_format": "2006-01-02 15:04:05"
          }
        },
        {
		  "type": "battery",
          "style": "diamond",
          "foreground": "#222222",
          "background": "#EF9355",
          "leading_diamond": "\uE0D4",
          "trailing_diamond": "\uE0CC\uE0D4\uE0B0",
          "properties": {
            "postfix": "% ",
            "charging_icon": "\uF583",
            "discharging_icon": "\uF58B",
            "charged_icon": "\uF578",
            "color_background": true,
            "charged_color": "#3FC56B",
            "charging_color": "#CACA02",
            "discharging_color": "#D3D7CF",
            "display_charging": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "root",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#4691E8",
          "properties": {
            "prefix": "<#4691E8,transparent>\uE0B6</>",
            "postfix": "<transparent,#4691E8>\uE0B2</>",
            "root_icon": "\uF085 "
          }
        },
        {
          "type": "session",
          "style": "diamond",
          "foreground": "#DCDCCC",
          "background": "#424242",
          "leading_diamond": "\uE0B2",
          "trailing_diamond": "\uE0B0",
          "properties": {
            "prefix": "<#7CFC00,>\uF872</> ",
            "postfix": "",
            "user_color": "#DF8FCD",
            "host_color": "#6CC1DF",
            "user_info_separator": " @ "
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "foreground": "#193549",
          "background": "#95ffa4",
          "powerline_symbol": "\uE0B0",
          "properties" : {
            "prefix": "\uF414 ",
            "postfix": "",
            "home_icon": "\uF015 ",
            "folder_icon": "\uF413",
            "folder_separator_icon": "\u27E9",
            "style": "full",
			"enable_hyperlink": true,
			"stack_count_enabled": true
          }
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#100e23",
          "background": "#91ddff",
          "powerline_symbol": "\uE0B0",
          "properties": {
            "prefix": "\uF7A1 ",
            "postfix": "",
            "display_status": true,
            "display_stash_count": true,
            "display_upstream_icon": true,
            "status_colors_enabled": true,
            "color_background": false,
            "local_changes_color": "",
            "working_color": "#E84855",
            "staging_color": "#2FDA4E",
            "ahead_color": "#2EC4B6",
            "behind_color": "#8A4FFF",
            "git_icon": "\uE255",
            "commit_icon": "\uF417 "
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "shell",
          "style": "plain",
          "foreground": "#59C718",
          "properties": {
            "prefix": " <#757575,>\uE20F</> ",
            "postfix": ""
          }
        },
        {
          "type": "root",
          "style": "plain",
          "foreground": "#ff8080",
          "properties": {
            "prefix": " <#757575,>as</> ",
            "postfix": "",
            "root_icon": "root"
          }
        },
        {
          "type": "text",
          "style": "powerline",
          "foreground": "#6AD2FF",
          "background": "#5A6D7A",
          "powerline_symbol": "\uE0B0",
          "properties": {
            "prefix": "",
            "postfix": " \uE0B1",
            "text": "\uF68C"
          }
        }
      ]
    }
  ]
}
