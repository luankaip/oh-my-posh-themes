    },
    {
      "type": "prompt",
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "shell",
          "style": "plain",
          "foreground": "#59C718",
          "properties": {
            "prefix": " <#757575,>\u21B3</>",
